{
  "name": "quick-yaml.db",
  "version": "1.2.2",
  "description": "An easy, open-source, Node.js database based on YAML files.",
  "main": "lib/cjs/index.js",
  "typings": "typings/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/es6/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./lib/cjs/*": "./lib/cjs/*.js",
    "./lib/es6/*": "./lib/es6/*.js"
  },
  "scripts": {
    "test": "node .",
    "compile-es6": "tsc --project tsconfig.es6.json",
    "compile-cjs": "tsc --project tsconfig.cjs.json"
  },
  "keywords": [
    "js-yaml",
    "js-yml",
    "node yaml",
    "node yml",
    "yaml",
    "yml",
    "yaml-db",
    "yaml db",
    "yml-db",
    "yml db",
    "yaml database",
    "yml database"
  ],
  "author": "TFAGaming",
  "license": "GPL-3.0",
  "dependencies": {
    "js-yaml": "^4.1.0"
  },
  "repository": {
    "url": "https://github.com/TFAGaming/quick-yaml.db.git"
  },
  "homepage": "",
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.6",
    "undici-types": "^6.3.0"
  }
}
